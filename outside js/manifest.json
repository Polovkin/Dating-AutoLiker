{
  "name": "Datind AutoLiker",
  "description" : "Autolike on mamba.ru , fotostrana.ru",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popout.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "https://mamba.ua/"
  ],
  "manifest_version": 2
}
